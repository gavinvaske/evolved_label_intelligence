{
  "name": "nodejs_starter_template",
  "version": "1.0.0",
  "description": "An application used to get up and running with a new nodejs project with a linter, test library and basic configuration already setup",
  "main": "app/index.js",
  "engines": {
    "npm": "6.14.13",
    "node": "14.17.0"
  },
  "scripts": {
    "start": "node application/index.js",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint .",
    "fix-lint": "./node_modules/.bin/eslint . --fix",
    "verify": "npm run test && npm run lint",
    "nodemon": "nodemon application/index.js"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "4.4.1"
    }
  },
  "author": "Gavin Vaske",
  "license": "ISC",
  "devDependencies": {
    "chance": "1.1.8",
    "eslint": "8.8.0",
    "jest": "27.5.0",
    "nodemon": "2.0.15"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.6",
    "dotenv": "16.0.0",
    "ejs": "3.1.6",
    "express": "4.17.2",
    "express-ejs-layouts": "2.5.1",
    "express-session": "1.17.2",
    "form-data": "4.0.0",
    "jsonwebtoken": "8.5.1",
    "mailgun.js": "4.2.2",
    "mongodb-memory-server": "8.3.0",
    "mongoose": "6.2.4",
    "multer": "1.4.4",
    "xml2json": "0.12.0"
  }
}
