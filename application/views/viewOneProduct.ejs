    <div class="alert-box full-width">
        <%- include ("flashMessages") %>
    </div>

    <div class="card">
        <h1>Order Workflow - <%= ticket.destination && ticket.destination.department || 'N/A' %> - #<%= ticket.ticketNumber || 'N/A' %> - <%= product.productNumber || 'N/A' %> - <%= ticket.customerName || 'N/A' %> </h1>
        <p>Description: <%= product.description || 'N/A' %> (Storm: Should the description default to 'N/A' if it doesn't exist?)</p>
