<!-- Order Prep - Needs Attention -->
<div id='' class='table-row-wrapper full-width order-prep-needs-attention-row' style="display: none;">
  <%- include('start-job-modal.ejs', {
    ticket: {}
    }
  ) %>
  <div class='table-row flex-center-center-column'>
    <div class='flex-center-center-row ticket-wrapper full-width'>
      <div class='column-td column-td-a'>
        <i class='fa-light fa-ellipsis-vertical text-white'></i>
        <%- include('ticket-dropdown-options.ejs', {
          ticket: {}
          }
        ) %>
      </div>
      <div class='column-td column-td-b bg-white row-number'></div>
      <div class='column-td column-td-c bg-white ticket-number-column'>#</div>
      <div class='column-td column-td-d bg-white status-column'></div>
      <div class='column-td colum11n-td-e bg-white'></div>
      <div class='column-td column-td-f bg-white'></div>
      <div class='column-td column-td-g bg-white'></div>
      <div class='column-td column-td-h bg-white'>
        <div class='user-cell bg-blue tooltip assignee-picture-url-column'>SV
          <span class="tooltiptext assignee-name-column">TODO</span>
        </div><span class='department-name department-column'>N/A</span>
      </div>
      <div class='column-td column-td-i bg-white due-date-preview due-date-column'>July 4</div>
    </div>
  </div>
  <%- include('products-wrapper.ejs', {
    ticket: {}
    }
  ) %>
</div>

